{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":assignee(dsegurag)",
    ":automergePatch",
    ":disableDependencyDashboard",
    ":disableRateLimiting",
    ":pinDigestsDisabled",
    ":semanticCommits"
  ],
  "ignorePresets": [
    ":combinePatchMinorReleases"
  ],
  "gitAuthor": "Renovate Bot <bot@renovateapp.com>",
  "onboardingConfig": {
    "extends": [
      "github>dsegurag/renovate-bot"
    ]
  },
  "timezone": "Europe/Madrid",
  "labels": [
    "renovate"
  ],
  "major": {
    "addLabels": [
      "major"
    ]
  },
  "minor": {
    "addLabels": [
      "minor"
    ]
  },
  "patch": {
    "addLabels": [
      "patch"
    ]
  },
  "pin": {
    "addLabels": [
      "pin"
    ]
  },
  "digest": {
    "addLabels": [
      "digest"
    ]
  },
  "repositories": [
    "dsegurag/renovate-bot",
    "dsegurag/aws-basic-network"
  ]
}
